depend() {
	need reducecap
}

start() {
	ebegin "Starting mdadmd"
	start-stop-daemon --start --exec /sbin/mdadmd
	eend $?
}

stop() {
	ebegin "Stopping mdadmd"
	start-stop-daemon --stop --name mdadmd
	eend $?
}
